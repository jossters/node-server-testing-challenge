{
  "name": "node-server-testing",
  "version": "1.0.0",
  "description": "Node Server Testing Challenge",
  "scripts": {
    "test": "jest --watch",
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrateup": "knex migrate:up",
    "migratedown": "knex migrate:down",
    "latest": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "reset": "npm run rollback && npm run latest && npm run seed",
    "cleanup": "knex seed:run --specific=01-cleanup.js",
    "seed": "knex seed:run"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LambdaSchool/node-server-testing.git"
  },
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.13.0",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "supertest": "^6.0.1",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "knex-cleaner": "^1.3.1",
    "sqlite3": "^5.0.2"
  }
}
